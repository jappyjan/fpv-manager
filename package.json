{
  "name": "fpv-manager",
  "private": true,
  "version": "1.0.0",
  "description": "FPV Manager",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production tsc && vite build && npx workbox generateSW workbox-config.js",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/",
    "start:backend": "cd backend && bun install && bun run index.ts"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@capacitor/android": "^5.4.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/browser": "^5.1.0",
    "@capacitor/core": "^5.4.0",
    "@capacitor/ios": "^5.4.0",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "dom7": "^4.0.6",
    "firebase": "^10.4.0",
    "framework7": "^8.3.0",
    "framework7-icons": "^5.0.5",
    "framework7-react": "^8.3.0",
    "material-icons": "^1.13.11",
    "printd": "^1.6.0",
    "prop-types": "^15.8.1",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxdb": "^14.17.1",
    "rxdb-hooks": "^5.0.2",
    "rxjs": "^7.8.1",
    "short-unique-id": "^5.0.2",
    "skeleton-elements": "^4.0.1",
    "swiper": "^10.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.4.0",
    "@types/crypto-js": "^4.1.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss-preset-env": "^9.1.4",
    "typescript": "^5.0.2",
    "vite": "^4.4.9",
    "vite-compatible-readable-stream": "^3.6.1",
    "vite-plugin-node-polyfills": "^0.14.1",
    "workbox-cli": "^7.0.0"
  }
}
